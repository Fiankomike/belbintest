<!-- ########  Parliamentary VOTE COUNT ###### -->
	<!-- Header -->
	<div class="row">
			<h1 class="text-center">Parliamentary Vote Count</h1>
	</div>

	<div class="row">
			<div class="table-responsive" style="margin-bottom: -15px;">
				<table class="table table-striped table-hover">
		  			<tr>
		  				<th>Party</th>
		  				<th>Total Votes </th>
		  			</tr>
		  		</table>
		  	</div>
	 </div>
		<!-- Parliamentary Table -->
	
	<div class="mygrid-wrapper-div">
			<div class="table-responsive">
				<table class="table table-striped table-bordered table-hover" style="margin-top: -40px;">
		  			<tr style="visibility: hidden;">
		  				<th>Party</th>
		  				<th>Total Votes </th>
		  			</tr>

		  	<% @pmttotal.each do |smsvotes| %>

		  			<tr>
		  				<td><%=smsvotes[0] %></td>
		  				<td><%=smsvotes[1] %></td>
		  			</tr>

		  		<% end %>
		
				</table>
			</div>
	</div>